{
  "name": "react-patterns",
  "version": "3.0.3",
  "description": "Minimal Pattern Library (style guide) React component for displaying HTML and CSS",
  "main": "./build/index.js",
  "scripts": {
    "build": "babel src --out-file build/index.js",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/springload/react-patterns/"
  },
  "keywords": [
    "style",
    "guide",
    "pattern",
    "library",
    "html",
    "css",
    "react",
    "jsx",
    "sass",
    "less"
  ],
  "author": "Matthew Holloway",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "benchmark": "^2.1.4",
    "jest": "^21.0.2",
    "memwatch-next": "^0.3.0",
    "react": ">15",
    "react-test-renderer": ">15"
  },
  "peerDependencies": {
    "react": ">15"
  },
  "babel": {
    "presets": [
      "flow",
      [
        "env",
        {
          "targets": {
            "node": "3"
          }
        }
      ],
      "react"
    ],
    "plugins": [
      [
        "transform-react-jsx"
      ],
      [
        "transform-object-rest-spread"
      ]
    ]
  },
  "dependencies": {
    "css-zero-beautify": "^2.0.5",
    "file-saver": "^1.3.8",
    "jszip": "^3.1.5",
    "xml-zero-beautify": "^2.0.5"
  }
}
